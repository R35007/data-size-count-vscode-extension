{
	"name": "data-size-count",
	"displayName": "Data Size Count",
	"description": "Shows the selected JSON object or array size, lines count, words count",
	"version": "0.0.6",
	"icon": "images/thinker.png",
	"publisher": "Thinker",
	"license": "MIT",
	"author": {
		"name": "Sivaraman",
		"email": "sendmsg2siva@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/R35007/data-size-count-vscode-extension"
	},
	"keywords": [
		"selected",
		"lines",
		"count",
		"size",
		"length",
		"data",
		"words",
		"status"
	],
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint"
	},
	"devDependencies": {
		"@types/vscode": "^1.30.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"typescript": "^4.0.2"
	},
	"dependencies": {
		"durable-json-lint": "0.0.3"
	}
}